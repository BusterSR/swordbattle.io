{
  "name": "swordbattle.io",
  "version": "2.0.0",
  "description": "A multiplayer sword fighting game",
  "scripts": {
    "heroku-prebuild": "npm install --dev",
    "build": "webpack --config webpack/prod.js ",
    "watch": "webpack --config webpack/base.js --watch --progress",
    "start-depricated": "webpack-dev-server --config webpack/base.js --open",
    "start": "npx tsc && node ./dist-server/server/server.js",
    "fix": "prettier --write './src/client/**/*.js' && eslint --fix './src/client/**/*.js'",
    "style": "stylelint --fix './src/client/**/*.js'",
    "dev": "concurrently \"npm run watch\" \"npm run start\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codergautam/swordbattle.io.git"
  },
  "keywords": [
    "phaser",
    "react",
    "multiplayer",
    "io-game",
    "io",
    "game",
    "websockets",
    "swordbattle"
  ],
  "author": "codergautam",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/codergautam/swordbattle.io/issues"
  },
  "homepage": "https://github.com/codergautam/swordbattle.io#readme",
  "dependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@timohausmann/quadtree-js": "^1.2.5",
    "@types/intersects": "^2.5.0",
    "babel-loader": "^9.1.2",
    "buffer": "^6.0.3",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.6.0",
    "css-loader": "^6.7.3",
    "html-webpack-plugin": "^5.5.0",
    "intersects": "^2.7.2",
    "phaser": "^3.55.2",
    "phaser3-rex-plugins": "^1.1.80",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "style-loader": "^3.3.1",
    "uWebSockets.js": "uNetworking/uWebSockets.js#v20.19.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "devDependencies": {
    "typescript": "^4.9.5"
  }
}
